{
  "name": "@metarhia/sql",
  "version": "0.0.0",
  "description": "Metarhia SQL utilities",
  "main": "sql.js",
  "scripts": {
    "lint": "eslint . && remark . && prettier --check \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.yml\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.yml\"",
    "test": "npm run -s lint && metatests test/",
    "doc": "metadoc -c .metadocrc && npm run fmt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metarhia/sql.git"
  },
  "keywords": [
    "SQL",
    "Metarhia"
  ],
  "author": "Denys Otrishko <shishugi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metarhia/sql/issues"
  },
  "homepage": "https://github.com/metarhia/sql#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@metarhia/doc": "^0.2.1",
    "eslint": "^5.15.3",
    "eslint-config-metarhia": "^7.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "metatests": "^0.6.0",
    "prettier": "^1.16.4",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-metarhia": "^1.0.4",
    "remark-validate-links": "^8.0.2"
  },
  "dependencies": {
    "@metarhia/common": "^1.3.0"
  }
}
